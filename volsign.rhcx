# Volume Signature
# Runnable Hermes Core eXtension

export CXS_AUTOGEN_FILENAME="lib/hermes/defaults/nvram/VFVSG/systemroot.hcx"

export CXS_ENTITLEMENT="SYSTEM"

export CERT_MANUFACTURE="410"

export CXS_SIGNSEED="$(md5 -qs "OS=Hermes;MAJOR=10;MANUFACTURE=$CERT_MANUFACTURE")"
export CXS_CODESIGN="$(echo "entitlement=$CXS_ENTITLEMENT;signseed=$CXS_SIGNSEED" | shasum -a 512)"


echo "$CXS_CODESIGN" > "./$CXS_AUTOGEN_FILENAME"
