#!/bin/bash

# DO NOT EDIT THIS FILE!!
# This is a partition location table file that bootloader reads to correclty locate the system and its file.
# If this file is edited, the system may not load properly.

export ROOTFS="/Volumes/VFS"

export SYSTEM="$ROOTFS/System"
export CACHE="$ROOTFS/cache"
export DATA="$ROOTFS/Data"

export TDLIB="$SYSTEM/TouchDown"
export BOOTSECT="$SYSTEM/boot"
export SBIN="$SYSTEM/sbin"

export USERDATA="$DATA/User"
export LIB="$DATA/Library"
export MULTIPLEX="$DATA/config"

export NVRAM="$LIB/nvram"