#!/bin/bash

if [[ -z "$(FIRMWARE_INFO 2>/dev/null)" ]] || [[ $(bootArgumentHas "no-firm-support") == "1" ]]; then
	function FIRMWARE_INFO() {
		if [[ "$1" == "version" ]]; then
			echo "None"
		elif [[ "$1" == "manufacture" ]]; then
			echo "None"
		else
			echo "Version: None"
			echo "Manufacture: None"
		fi
	}

	export -f FIRMWARE_INFO
fi